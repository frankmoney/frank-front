{
  "name": "frank.frank-front",
  "version": "0.0.1",
  "main": "bin/http.js",
  "repository": "https://github.com/frankmoney/frank-front",
  "author": "Frank Team",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=\"production\" SSR=true node .",
    "dev": "cross-env ASSETS_PATH=/assets/ WIDGET_SCRIPT_URL=https://assets.frank.ly node .",
    "dev-widget": "cross-env ASSETS_PATH=/assets/ API_URL=http://back.frank-dev1.frank.ly WEBAPP_URL=https://frank-dev1.frank.ly webpack-dev-server --config ./webpack.config.widget.dev.js",
    "dev-ssr": "cross-env SSR=true npm run dev",
    "test": "jest",
    "test:watch": "npm run test --watch",
    "test:coverage": "npm run test --coverage",
    "build:server": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.server.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "build": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.prod.js",
    "build-widget": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.widget.prod.js",
    "deploy": "gulp gcloud-webapp",
    "deploy:widget": "gulp gcloud-widget",
    "flow": "flow --show-all-errors",
    "flow-typed": "flow-typed",
    "lint-fix": "find ./src -name \"*.js[xs]\" | xargs eslint --fix",
    "lint": "eslint src --ext js,jsx,jss --cache && echo \"eslint: no lint errors\""
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "jss"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-7-jest"
    },
    "moduleNameMapper": {
      "^test/([a-zA-Z]+)$": "<rootDir>/test/$1",
      "^test/utils/([a-zA-Z]+)$": "<rootDir>/test/utils/$1",
      "^.+\\.jss$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "pre-push": [],
  "devDependencies": {
